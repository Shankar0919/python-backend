collection:
  info:
    name: Python Backend API
    version: 1.0.0
requests:
- name: GET /healthcheck
  method: GET
  url: http://localhost:5000/healthcheck
- name: POST /api/users/create
  method: POST
  url: http://localhost:5000/api/users/create
- name: PUT /api/users/update/<user_id>
  method: PUT
  url: http://localhost:5000/api/users/update/<user_id>
- name: DELETE /api/users/delete/<user_id>
  method: DELETE
  url: http://localhost:5000/api/users/delete/<user_id>
